#include <stdio.h>

int main()
{ 
    int n;
    printf("enter the no of element in array:\n");
    scanf("%d",&n);

    int i,arr[n],t;
    for(i=0;i<n;i++)
    {  
        printf("enter the array element\n");
        scanf("%d",&arr[i]);
    }
    
    for(i=0;i<n;i++)
    {
        t=arr[0];
        if(arr[i]>t)
        {
        
        t=arr[i];
            
        }
        

    }
    printf("%d",t);
    return 0;
}
    
    
    
    
    
    
    
    
    
    
    
    
    
