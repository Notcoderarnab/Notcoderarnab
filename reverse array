#include<stdio.h>
int reversearr(int arr[],int n);
void printarr(int arr[],int n);
int main()
    {
        int i;
        int arr[]={1,2,3};
        reversearr(arr,3);
        printarr(arr,3);

        return 0;

    }
int reversearr( int arr[],int n)
{int i;
        for(i=0;i<n/2;i++){
       int first =arr[i];
       int second=arr[n-i-1];
        arr[i]=second;
        arr[n-i-1]=first;

        }
}
void printarr(int arr[],int n)
{ int i;
    for(i=0;i<n;i++)
    {
    printf("%d",arr[i]);
    }

}


